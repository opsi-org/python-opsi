iaStor_ICH8RICH9RICH10RDO = "Intel(R) ICH8R/ICH9R/ICH10R/DO SATA RAID Controller"
iaStor_ICH8MEICH9ME       = "Intel(R) ICH8M-E/ICH9M-E SATA RAID Controller"

